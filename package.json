{
  "name": "chrome-debugging-client",
  "version": "1.0.0-beta",
  "description": "An async/await friendly Chrome debugging client with TypeScript support",
  "keywords": [
    "CDP",
    "chrome",
    "debugging",
    "protocol",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/devtrace/chrome-debugging-client.git"
  },
  "license": "BSD-2-Clause",
  "author": "Kris Selden <kris.selden@gmail.com>",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "!dist/test",
    "index.ts",
    "lib/*.ts",
    "protocol/*.ts",
    "codegen/*.ts"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "codegen": "dist/codegen",
    "dist": "dist",
    "protocol": "dist/protocol"
  },
  "scripts": {
    "compile-protocol-gen": "tsc -p scripts/tsconfig.json",
    "lint": "tslint -p tsconfig.json",
    "prepare": "tsc",
    "protocol-gen": "yarn compile-protocol-gen && node dist/scripts/generate-protocol.js",
    "test": "ava"
  },
  "dependencies": {
    "chrome-launcher": "^0.10.2",
    "debug": "^4.0.1",
    "devtools-protocol": "*",
    "execa": "^1.0.0",
    "tmp": "^0.0.33",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "@types/tmp": "^0.0.33",
    "@types/ws": "*",
    "@types/debug": "0.0.30",
    "@types/execa": "^0.9.0",
    "@types/got": "^8.3.3",
    "@types/node": "^10.9.2",
    "@types/tape": "^4.2.32",
    "ava": "^0.25.0",
    "devtools-protocol": "^0.0.589586",
    "got": "^9.2.0",
    "prettier": "^1.14.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.3"
  },
  "engine": "node >= 4.0.0"
}
